{
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.fontFamily": "Firacode Nerd Font",
  "editor.tabCompletion": "on",
  "git.autofetch": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.cursorBlinking": "smooth",
  "editor.inlineSuggest.enabled": true,
  "code-runner.executorMap": {
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "groovy": "groovy",
    "perl6": "perl6",
    "ruby": "ruby",
    "javascript": "node",
    "shellscript": "bash",
    "vbscript": "cscript //Nologo",
    "scala": "scala",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "swift": "swift",
    "clojure": "lein exec",
    "racket": "racket",
    "ahk": "autohotkey",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "dart": "dart",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "coffeescript": "coffee",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "perl": "perl",
    "lisp": "sbcl --script",
    "csharp": "scriptcs",
    "ocaml": "ocaml",
    "fsharp": "fsi",
    "applescript": "osascript",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "go": "go run",
    "bat": "cmd /c",
    "python": "python3 -u",
    "crystal": "crystal",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "r": "Rscript",
    "scheme": "csi -script",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "haskell": "runhaskell",
    "v": "v run",
    "lua": "lua",
    "php": "php",
    "julia": "julia",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "autoit": "autoit3",
    "kit": "kitc --run",
    "typescript": "ts-node"
  },
  "explorer.confirmDelete": false,
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "terminal.integrated.cursorStyle": "line",
  "liveServer.settings.donotShowInfoMsg": true,
  "files.autoSave": "onFocusChange",
  "helium-icon-theme.folders.color": "#92b3f5",
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground2": "#B3E1A7",
    "editorBracketHighlight.unexpectedBracket.foreground": "#E48FA8",
    "editorBracketHighlight.foreground1": "#EFB68E",
    "editorBracketHighlight.foreground4": "#C5A7F2",
    "editorBracketHighlight.foreground3": "#F5E3B5"
  },
  "workbench.fontAliasing": "antialiased",
  "terminal.integrated.cursorBlinking": true,
  "editor.bracketPairColorization.enabled": true,
  "window.dialogStyle": "custom",
  "terminal.external.osxExec": "iTerm2.app",
  "editor.accessibilitySupport": "off",
  "editor.linkedEditing": true,
  "helium-icon-theme.saturation": 1,
  "workbench.productIconTheme": "fluent-icons",
  "monkeyPatch.browserModules": [],
  "editor.smoothScrolling": true,
  "editor.guides.bracketPairs": true,
  "explorer.openEditors.sortOrder": "alphabetical",
  "editor.fontWeight": "normal",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.list.smoothScrolling": true,
  "editor.inlayHints.fontFamily": "Firacode Nerd Font",
  "editor.fontLigatures": true,
  "explorer.openEditors.visible": 40,
  "editor.fontFamily": "Firacode Nerd Font",
  "workbench.iconTheme": "catppuccin-perfect-mocha",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "settingsSync.ignoredSettings": [
    "terminal.integrated.fontFamily",
    "editor.fontFamily",
    "debug.console.fontFamily"
  ],
  "editor.cursorSmoothCaretAnimation": "on",
  "indentRainbow.colors": [
    "#f5c2e722",
    "#cba6f722",
    "#f38ba822",
    "#fab38722",
    "#f9e2af22",
    "#a6e3a122",
    "#94e2d522",
    "#89dceb22",
    "#74c7ec22",
    "#89b4fa22"
  ],

  // The indent color if the number of spaces is not a multiple of "tabSize".

  // The indent color when there is a mix between spaces and tabs.
  // To be disabled this coloring set this to an empty string.
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "Wscats.eno"
  },
  "workbench.editor.enablePreview": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.formatOnType": true
  },
  "editor.minimap.renderCharacters": false,
  "catppuccin.accentColor": "blue",
  "editor.fontSize": 14,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "git.enableSmartCommit": true,
  "update.mode": "start",
  "git.confirmSync": false,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "vscode.css-language-features",
  "applescript.alwaysOpenBuildTask": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.minimap.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "mblode.pretty-formatter"
  },
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "tailwind-fold.unfoldIfLineSelected": true,
  "redhat.telemetry.enabled": false,
  "git-graph.date.format": "Relative",
  "git-graph.graph.colours": [
    "#f2cdcd",
    "#f5c2e7",
    "#cba6f7",
    "#f38ba8",
    "#fab387",
    "#f9e2af",
    "#a6e3a1",
    "#94e2d5",
    "#89dceb",
    "#74c7ec",
    "#89b4fa",
    "#b4befe"
  ],
  "window.zoomLevel": 1,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "window.commandCenter": false,
  "apc.electron": {
    // "titleBarStyle": "hidden",
    "frame": false
  },
  "apc.statusBar": {
    "position": "editor-top",
    "height": 20,
    "fontSize": 12
  },
  "apc.imports": ["/Users/tnixc/.config/vscode/custom.css"],
  "workbench.editor.untitled.labelFormat": "name",
  "window.titleBarStyle": "native",
  "apc.font.family": "SF Pro",
  "editor.codeLensFontFamily": "Firacode Nerd Font",
  "debug.console.fontFamily": "Firacode Nerd Font",
  "scm.inputFontFamily": "Firacode Nerd Font",
  "apc.monospace.font.family": "Firacode Nerd Font"
}
